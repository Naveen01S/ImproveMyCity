# Environment Configuration for Improve My City App

# Supabase Configuration (Current Backend)
VITE_SUPABASE_URL=your_supabase_url_here
VITE_SUPABASE_ANON_KEY=your_supabase_anon_key_here

# Firebase Configuration (Authentication)
VITE_FIREBASE_API_KEY=your_firebase_api_key_here
VITE_FIREBASE_AUTH_DOMAIN=your_project_id.firebaseapp.com
VITE_FIREBASE_PROJECT_ID=your_project_id
VITE_FIREBASE_STORAGE_BUCKET=your_project_id.appspot.com
VITE_FIREBASE_MESSAGING_SENDER_ID=your_sender_id
VITE_FIREBASE_APP_ID=your_app_id

# Google Maps API
VITE_GOOGLE_MAPS_API_KEY=your_google_maps_api_key_here

# Cloudinary Configuration (Image Storage)
VITE_CLOUDINARY_CLOUD_NAME=your_cloudinary_cloud_name
VITE_CLOUDINARY_API_KEY=your_cloudinary_api_key
VITE_CLOUDINARY_API_SECRET=your_cloudinary_api_secret

# OpenAI Configuration (AI Features)
VITE_OPENAI_API_KEY=your_openai_api_key_here

# MongoDB Configuration (Database)
VITE_MONGODB_URI=mongodb://localhost:27017/improve-my-city
VITE_MONGODB_DATABASE=improve-my-city

# App Configuration
VITE_APP_NAME=Improve My City
VITE_APP_VERSION=1.0.0
VITE_APP_ENVIRONMENT=development

